{
  "name": "react_node.js_webpack",
  "version": "1.0.0",
  "main": "dist/server.bundle.js",
  "scripts": {
    "start": "node dist/server.bundle.js",
    "build": "run-p build:client build:server",
    "build:client": "webpack --mode production --progress --color --devtool false",
    "build:server": "webpack --mode production --progress --colors --devtool false --config webpack.config.server.js",
    "dev:server:build": "webpack --mode development --colors --config webpack.config.server.js",
    "dev:client": "webpack-dev-server --mode development --devtool inline-source-map --hot --colors --progress",
    "dev:server:run": "nodemon",
    "dev:server": "run-p dev:server:build dev:server:run",
    "dev": "run-p dev:server dev:client"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "absolute"
    }
  },
  "author": "phchu",
  "license": "ISC",
  "private": true,
